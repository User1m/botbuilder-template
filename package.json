{
  "name": "botbuilder-template",
  "version": "1.0.0",
  "description": "botbuilder-template",
  "main": "./bundle",
  "pre-commit": [
    "test",
    "lint"
  ],
  "scripts": {
    "build": "gulp clean transpile && npm run pack",
    "build-start": "npm run build && npm run start",
    "check-security": "gulp check-security",
    "ci-test": "mocha-typescript-watch -p tsconfig.json ./release/test/",
    "clean": "gulp clean",
    "console": "./node_modules/.bin/nodemon --delay 200ms ./release/console",
    "deploy": "npm cache clean",
    "debug-test": "mocha --inspect-brk ./release/test/",
    "enforce-code-cov": "gulp enforce-code-coverage",
    "gulp": "gulp",
    "lint": "gulp tslint",
    "jasmine": "node ./release/tests/specs/runner",
    "pack": "webpack",
    "package": "gulp package-transpiled-javascript",
    "server": "./node_modules/.bin/nodemon --delay 200ms ./release/server",
    "show-cov-ut": "gulp show-unittest-coverage-report",
    "start": "node ./bundle",
    "show-npm-packages": "npm ls",
    "test": "npm run jasmine && gulp run-unit-tests check-security",
    "transpile": "gulp transpile",
    "watch": "tsc -w -p tsconfig.json"
  },
